<!-- include Head -->
  <%- include('./partials/head') %>

    <body id="page-top">
        <!-- include Navigation-->
        <%- include('./partials/navbar') %>

        <!-- Masthead-->
        <header class="masthead">
            <div class="container d-flex h-100 align-items-center">
                <div class="mx-auto text-center">
                    <h1 class="mx-auto my-0 text-uppercase"><%= __('The best deals are here')%></h1>
                    <h2 class="text-white-50 mx-auto mt-2 mb-5"><%= __('You can sell and buy new or used products and finding the best deal for you')%></h2>
                    <a class="btn btn-primary js-scroll-trigger" href="https://documenter.getpostman.com/view/12399642/TzRNE9xB"><%= __('Get Started')%></a>
                </div>
            </div>
        </header>

        <!-- Projects: Articles -->
        <section class="projects-section bg-light" id="articles">
          <div class="container">
              <!-- Featured Project Row-->
                <table class="table table-striped">
                  <thead>
                    <tr>
                      <th scope="col">#</th>
                      <th scope="col"><%= __('Name')%></th>
                      <th scope="col"><%= __('Operation')%></th>
                      <th scope="col"><%= __('Price')%></th>
                      <th scope="col"><%= __('Tags')%></th>
                    </tr>
                  </thead>
                  <tbody>
                    <tr>
                      <th scope="row">1</th>
                      <td>Xbox One</td>
                      <td><%= __('Sell')%></td>
                      <td>$500</td>
                      <td><%= __('LifeStile')%></td>
                    </tr>
                    <tr>
                      <th scope="row">2</th>
                      <td>iPhone 12</td>
                      <td><%= __('Buy')%></td>
                      <td>$1000</td>
                      <td><%= __('Mobile')%></td>
                    </tr>
                    <tr>
                      <th scope="row">3</th>
                      <td>Dean Electric Guitar Dimebag</td>
                      <td><%= __('Sell')%></td>
                      <td>$799</td>
                      <td><%= __('Musical')%></td>
                    </tr>
                    <tr>
                      <th scope="row">4</th>
                      <td>Ford Mustang 1966 Convertible</td>
                      <td><%= __('Sell')%></td>
                      <td>$105000</td>
                      <td><%= __('Motor')%></td>
                    </tr>
                    <tr>
                      <th scope="row">5</th>
                      <td>Orbea Bycicle M20 2019</td>
                      <td><%= __('Buy')%></td>
                      <td>$3500</td>
                      <td><%= __('Mobility')%></td>
                    </tr>
                  </tbody>
                </table>
              </div>
        </section>
        <!-- include CDN Scripts -->
        <%- include('./partials/cdnScripts') %>

        <!-- include Footer -->
        <%- include('./partials/footer') %>
    </body>
</html>
